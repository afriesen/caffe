train_net: "${EXP}/config/${NET_ID}/train_${TRAIN_SET}.prototxt"

iter_size: 10
lr_policy: "poly"
power: 0.9
#base_lr: 2.5e-4
base_lr: 1.0e-4
#base_lr: 1.0e-6

average_loss: 20
display: 20
max_iter: 20000
momentum: 0.9
weight_decay: 0.0005
rms_decay: 0.0

#snapshot: 10000
snapshot: 2000
snapshot_prefix: "${EXP}/model/${NET_ID}/train${FOLD_SUFFIX}"
solver_mode: GPU
