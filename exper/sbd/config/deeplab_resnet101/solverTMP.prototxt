train_net: "${EXP}/config/${NET_ID}/trainTMP_${TRAIN_SET}.prototxt"

#iter_size: 10
iter_size: 1
lr_policy: "poly"
power: 0.9
#base_lr: 2.5e-4
#base_lr: 1.0e-30
base_lr: 1.0e-5

#average_loss: 20
average_loss: 1
display: 1
#max_iter: 20000
max_iter: 100000
#momentum: 0.9
momentum: 0.0
#weight_decay: 0.0005
weight_decay: 0
rms_decay: 0.0

snapshot: 10000
snapshot_prefix: "${EXP}/model/${NET_ID}/trainTMP${FOLD_SUFFIX}"
solver_mode: GPU
